{
  "name": "css-sorter",
  "displayName": "CSS Sorter",
  "description": "An extension that helps you organize your CSS.",
  "publisher": "zascal",
  "author": {
    "name": "Zsolti Domotor"
  },
  "repository": "https://github.com/dozsolti/CSS-sorter",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "css-sorter.sortByImportance",
        "title": "Sort CSS By Importance"
      },
      {
        "command": "css-sorter.sortAlphabetical",
        "title": "Sort CSS Alphabetical Order"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "group": "1_modification",
          "submenu": "css-sorter.subMenu",
          "when": "editorLangId =~ /^(css|scss|less|html)$/"
        }
      ],
      "css-sorter.subMenu": [
        {
          "command": "css-sorter.sortAlphabetical",
          "group": "css-sorter@1"
        },
        {
          "command": "css-sorter.sortByImportance",
          "group": "css-sorter@2"
        }
      ]
    },
    "submenus": [
      {
        "id": "css-sorter.subMenu",
        "label": "Sort CSS",
        "group": "css-sorter"
      }
    ]
  },
  "scripts": {
    "publish": "vsce publish",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.9",
    "css-declaration-sorter": "^7.2.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/postcss-less": "^4.0.6",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}
