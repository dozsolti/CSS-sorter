{
	"name": "CSS-sorter",
	"displayName": "CSS sorter",
	"description": "Extension which sorts your CSS/SCSS attributes.",
	"icon": "icon.png",
	"version": "0.2.0",
	"publisher": "zascal",
	"repository": "https://github.com/dozsolti/CSS-sorter",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.sortcss",
		"onCommand:extension.sortcssalphabetical"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.sortcss",
				"title": "Sort css by importance"
			},
			{
				"command": "extension.sortcssalphabetical",
				"title": "Sort css in alphabetical order"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == css",
					"command": "extension.sortcss",
					"group": "YourGroup@1"
				},
				{
					"when": "resourceLangId == scss",
					"command": "extension.sortcss",
					"group": "YourGroup@1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"cssjson": "^2.1.3"
	}
}
